# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Firebase_facebook' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Firebase_facebook
pod ‘FacebookCore’
pod ‘FacebookLogin’
pod ‘FBSDKCoreKit’
pod ‘FBSDKLoginKit’
pod ‘Firebase/Core’
pod ‘Firebase/Auth’
pod 'Kingfisher'

end
